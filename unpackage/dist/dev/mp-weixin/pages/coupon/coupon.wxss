/* 水平间距 */
.container {
  height: 1334rpx;
  background-color: #ecedef;
}
.container .navigationBar {
  height: 120rpx;
  background-color: #fff;
}
.container .navigationBar .item {
  width: 26%;
  color: #bcbcbc;
  float: left;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  position: relative;
}
.container .navigationBar .item .itemActive {
  position: absolute;
  bottom: 0;
  left: calc(50% - 46rpx);
  width: 80rpx;
  border: 6rpx solid #4288fd;
  border-radius: 6rpx;
}
.container .navigationBar .active {
  color: #4087fe;
}
.container .navigationBar .explain {
  width: 20%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 30rpx;
  float: right;
  text-align: center;
  border-left: 1px solid #eeeeee;
}
.container .navigationBar .explain ._img {
  width: 30rpx;
  height: 30rpx;
  position: relative;
  top: 5rpx;
  left: -10rpx;
}
.container .noneCoupon {
  width: 220rpx;
  height: 300rpx;
  margin: 200rpx auto;
}
.container .noneCoupon ._img {
  width: 220rpx;
  height: 200rpx;
}
.container .noneCoupon .text {
  font-size: 24rpx;
  color: #aaaaa9;
  margin-top: 50rpx;
  text-align: center;
}

