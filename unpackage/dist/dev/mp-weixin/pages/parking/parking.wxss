/* 水平间距 */
.container {
  position: relative;
}
.container ._h1,
.container ._h2 {
  text-align: center;
}
.container .floorSelect {
  text-align: center;
  width: 300rpx;
  border: 0px solid #fff;
}
.container .fatherView {
  width: 100%;
  overflow: hidden;
}
.container .fatherView .childView {
  width: 4000rpx;
  height: 4000rpx;
}
.container .fatherView .childView .map {
  position: relative;
}
.container .fatherView .childView .map .door {
  position: absolute;
}
.container .fatherView .childView .map .parking .item {
  position: absolute;
  width: 200rpx;
  height: 100rpx;
  border: 2rpx dashed #ccc;
}
.container .fatherView .childView .map .parking .select {
  background-color: aquamarine;
  color: #fff;
  text-align: center;
  line-height: 100rpx;
}
.container .fatherView .childView .map .parking .occupied {
  background-image: url(data:image/png;base64,);
  background-size: 200rpx 100rpx;
}
.container .fatherView .childView .canvas {
  width: 100%;
  height: 100%;
  z-index: -1;
}
.container .pay {
  position: fixed;
  width: 400rpx;
  height: 120rpx;
  line-height: 120rpx;
  bottom: 40rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  background-color: #872657;
  color: #fff;
  font-size: 40rpx;
  border-radius: 50rpx;
}

