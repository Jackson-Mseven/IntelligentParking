/* 水平间距 */
.container {
  background-color: #ebecee;
}
.container .prompt {
  padding: 20rpx 40rpx;
  font-size: 24rpx;
  color: #bbbdbb;
}
.container .carBind {
  width: 90%;
  height: 200rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto 40rpx;
  position: relative;
  display: flex;
  flex-wrap: wrap;
}
.container .carBind view {
  width: 45%;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}
.container .bind {
  width: 90%;
  height: 200rpx;
  background-color: #fff;
  border-radius: 10rpx;
  margin: 0 auto;
  position: relative;
}
.container .bind ._img {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: calc(50% - 30rpx);
  left: calc(50% - 30rpx);
}

